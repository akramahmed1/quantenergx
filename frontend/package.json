{
  "name": "quantenergx-frontend",
  "version": "1.0.0",
  "description": "Frontend application for QuantEnergx energy trading platform",
  "main": "src/index.tsx",
  "scripts": {
    "prestart": "node ../scripts/validate-env.js frontend",
    "start": "react-scripts start",
    "prebuild": "node ../scripts/validate-env.js frontend",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:security": "npm audit --audit-level=moderate && npm run lint:security",
    "test:lighthouse": "lhci autorun",
    "test:lighthouse:mobile": "lhci autorun --mobile",
    "test:lighthouse:desktop": "lhci autorun --desktop",
    "test:performance": "npm run test:lighthouse && npm run analyze",
    "test:performance-budget": "lhci autorun --assert",
    "test:web-vitals": "npx web-vitals-measure",
    "test:visual": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "test:visual-regression": "backstop test",
    "test:a11y": "pa11y-ci --sitemap http://localhost:3000/sitemap.xml",
    "test:bundle": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js --mode server",
    "eject": "react-scripts eject",
    "lint": "eslint src/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "lint:security": "ESLINT_USE_FLAT_CONFIG=false eslint src/ --ext .ts,.tsx --config .eslintrc.security.json",
    "lint:security:json": "ESLINT_USE_FLAT_CONFIG=false eslint src/ --ext .ts,.tsx --config .eslintrc.security.json --format json --output-file eslint-security-results.json",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "security:audit": "npm audit --audit-level=moderate",
    "security:scan": "npm run test:security",
    "test:smoke": "npm test -- --testNamePattern='smoke' --watchAll=false",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "react",
    "typescript",
    "energy-trading",
    "trading-platform",
    "commodities"
  ],
  "author": "QuantEnergx Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akramahmed1/quantenergx.git",
    "directory": "frontend"
  },
  "bugs": {
    "url": "https://github.com/akramahmed1/quantenergx/issues"
  },
  "homepage": "/",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.13.0",
    "@mui/material": "^5.13.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/dom": "^10.4.1",
    "@types/node": "^24.1.0",
    "chart.js": "^4.5.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.0",
    "react-scripts": "^0.0.0",
    "recharts": "^2.6.2",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/addon-onboarding": "^9.0.18",
    "@storybook/preset-create-react-app": "^1.2.0",
    "@storybook/react-webpack5": "^9.0.18",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "backstopjs": "^6.3.0",
    "chromatic": "^11.0.0",
    "eslint": "^8.40.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-security": "^3.0.1",
    "lighthouse": "^12.8.0",
    "pa11y-ci": "^4.0.0",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "storybook": "^9.0.18",
    "web-vitals": "^4.0.0",
    "webpack": "^5.100.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  }
}
