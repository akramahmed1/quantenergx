{
  "name": "quantenergx-backend",
  "version": "1.0.0",
  "description": "Backend services for QuantEnergx energy trading platform",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=test/unit",
    "test:integration": "jest --testPathPattern=test/integration",
    "test:fuzz": "jest --testPathPattern=test/fuzz",
    "test:contract": "jest --testPathPattern=test/contract",
    "test:regression": "../scripts/regression-test.sh",
    "test:mutation": "stryker run",
    "test:mutation:incremental": "stryker run --incremental",
    "test:all": "npm run lint && npm run test:coverage && npm run test:fuzz && npm run test:contract",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run"
  },
  "keywords": [
    "energy-trading",
    "commodities",
    "oil",
    "gas",
    "renewable-energy",
    "trading-platform"
  ],
  "author": "QuantEnergx Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akramahmed1/quantenergx.git",
    "directory": "backend"
  },
  "bugs": {
    "url": "https://github.com/akramahmed1/quantenergx/issues"
  },
  "homepage": "https://github.com/akramahmed1/quantenergx#readme",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.0",
    "redis": "^4.6.5",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "helmet": "^6.1.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "winston": "^3.8.2",
    "knex": "^2.5.1",
    "multer": "^2.0.0-rc.4",
    "tesseract.js": "^4.1.1",
    "pdf-parse": "^1.1.1",
    "sharp": "^0.32.1",
    "bull": "^4.10.4",
    "@grpc/grpc-js": "^1.8.14",
    "@grpc/proto-loader": "^0.7.7",
    "socket.io": "^4.6.2",
    "axios": "^1.4.0",
    "node-telegram-bot-api": "^0.61.0",
    "uuid": "^9.0.0",
    "moment": "^2.29.4",
    "validator": "^13.9.0",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "@pact-foundation/pact": "^15.0.1",
    "@stryker-mutator/core": "^9.0.1",
    "@stryker-mutator/jest-runner": "^9.0.1",
    "eslint": "^8.40.0",
    "fast-check": "^4.2.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  }
}